Options -MultiViews
RewriteEngine On
RewriteCond %{QUERY_STRING} os=windows
RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
RewriteCond %{QUERY_STRING} os=linux
RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]